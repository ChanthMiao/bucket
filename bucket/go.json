        }
    },
    "extension": "zip",
    "force_delete": [
        "${BAULK_VFS}\\go",
        "${BAULK_VFS}\\Temp\\go-build"
    ],
    "venv": {
        "category": "golang",
        "mkdir": [
            "${BAULK_VFS}\\Temp"
        ],
        "path": [
            "${BAULK_PKGROOT}\\bin",
            "${BAULK_VFS}\\go\\bin"
        ],
        "env": [
            "GOPATH=${BAULK_VFS}\\go",
            "GOENV=${BAULK_VFS}\\go\\env",
            "GOCACHE=${BAULK_VFS}\\Temp\\go-build",
            "GOTMPDIR=${BAULK_VFS}\\Temp",
            "GO111MODULE=on",
            "GO_PROXY=https://goproxy.io"
        ]
    }
}
