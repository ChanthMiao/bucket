{
    "description": "An open source programming language that makes it easy to build simple, reliable, and efficient software.",
    "version": "1.25.0",
    "homepage": "https://golang.org/",
    "license": "BSD",
    "architecture": {
        "64bit": {
            "url": "https://go.dev/dl/go1.25.0.windows-amd64.zip",
            "hash": "SHA256:89efb4f9b30812eee083cc1770fdd2913c14d301064f6454851428f9707d190b"
        },
        "arm64": {
            "url": "https://go.dev/dl/go1.25.0.windows-arm64.zip",
            "hash": "SHA256:27bab004c72b3d7bd05a69b6ec0fc54a309b4b78cc569dd963d8b3ec28bfdb8c"
        }
    },
    "extension": "zip",
    "force_delete": [
        "${BAULK_VFS}\\go",
        "${BAULK_VFS}\\Temp\\go-build"
    ],
    "venv": {
        "category": "golang",
        "mkdir": [
            "${BAULK_VFS}\\Temp"
        ],
        "path": [
            "${BAULK_PKGROOT}\\bin",
            "${BAULK_VFS}\\go\\bin"
        ],
        "env": [
            "GOPATH=${BAULK_VFS}\\go",
            "GOENV=${BAULK_VFS}\\go\\env",
            "GOCACHE=${BAULK_VFS}\\Temp\\go-build",
            "GOTMPDIR=${BAULK_VFS}\\Temp",
            "GO111MODULE=on",
            "GO_PROXY=https://goproxy.io"
        ]
    }
}
