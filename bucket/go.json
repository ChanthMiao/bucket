{
    "description": "An open source programming language that makes it easy to build simple, reliable, and efficient software.",
    "version": "1.16",
    "url": "https://dl.google.com/go/go1.16.windows-386.zip",
    "url.hash": "SHA256:481492a17d42193d471b93b7a06da3555331bd833b76336afc87be820c48933f",
    "url64": "https://dl.google.com/go/go1.16.windows-amd64.zip",
    "url64.hash": "SHA256:5cc88fa506b3d5c453c54c3ea218fc8dd05d7362ae1de15bb67986b72089ce93",
    "extension": "zip",
    "force_delete": [
        "${BAULK_VFS}\\go",
        "${BAULK_VFS}\\Temp\\go-build"
    ],
    "venv": {
        "category": "golang",
        "mkdir": [
            "${BAULK_VFS}\\Temp"
        ],
        "path": [
            "${BAULK_PKGROOT}\\bin",
            "${BAULK_VFS}\\go\\bin"
        ],
        "env": [
            "GOPATH=${BAULK_VFS}\\go",
            "GOENV=${BAULK_VFS}\\go\\env",
            "GOCACHE=${BAULK_VFS}\\Temp\\go-build",
            "GOTMPDIR=${BAULK_VFS}\\Temp",
            "GO111MODULE=on",
            "GO_PROXY=https://goproxy.io"
        ]
    }
}
